    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/creator"/>
    


    <!-- http://purl.org/dc/elements/1.1/rights -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/rights"/>
    


    <!-- http://purl.org/dc/elements/1.1/subject -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/subject"/>
    


    <!-- http://purl.org/dc/elements/1.1/title -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/title"/>
    


    <!-- http://purl.org/dc/terms/license -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/license"/>
    


    <!-- http://purl.org/dc/terms/title -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/title"/>
    


    <!-- http://purl.org/goodrelations/v1#legalName -->

    <owl:AnnotationProperty rdf:about="http://purl.org/goodrelations/v1#legalName"/>
    


    <!-- http://purl.org/goodrelations/v1#name -->

    <owl:AnnotationProperty rdf:about="http://purl.org/goodrelations/v1#name"/>
    


    <!-- http://purl.org/goodrelations/v1#offers -->

    <owl:AnnotationProperty rdf:about="http://purl.org/goodrelations/v1#offers"/>
    


    <!-- http://www.w3.org/2003/06/sw-vocab-status/ns#term_status -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2003/06/sw-vocab-status/ns#term_status"/>
    


    <!-- http://xmlns.com/foaf/0.1/homepage -->

    <owl:AnnotationProperty rdf:about="http://xmlns.com/foaf/0.1/homepage"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2001/XMLSchema#nonnegativeinteger -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#nonnegativeinteger"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/net/obop/actionInBlock -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/obop/actionInBlock">
        <rdfs:domain rdf:resource="http://purl.org/net/obop/BlockAction"/>
        <rdfs:range rdf:resource="http://purl.org/net/obop/Block"/>
        <rdfs:comment xml:lang="en">This object property specifies that an action manipulates data in the referenced block. For example, an instance of SubmitBlockAction is associated with a block (form) and represents the form&apos;s submit function.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/net/obop/v01"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/obop/activatesAction -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/obop/activatesAction">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://purl.org/net/obop/Action"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/net/obop/v01"/>
        <rdfs:label xml:lang="en">activatesAction</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/obop/activatesBranching -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/obop/activatesBranching">
        <rdfs:range rdf:resource="http://purl.org/net/obop/Branching"/>
        <rdfs:comment xml:lang="en">An interface element activates a branching statement which is used to deside what option should be chosen.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/obop/belongsTo -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/obop/belongsTo">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/net/obop/VisualElement"/>
        <rdfs:comment xml:lang="en-gb">The belongsTo property relates visual element or other elements to different
        entities, e.g., to blocks that represent HTML forms. </rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/net/obop/v01"/>
        <rdfs:label xml:lang="en">belongsTo</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/obop/belongsToCollection -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/obop/belongsToCollection">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/net/obop/CollectionItem"/>
        <rdfs:range rdf:resource="http://purl.org/net/obop/Collection"/>
        <rdfs:comment xml:lang="en">A belongsToCollection is an object property that relates a collection item and collection that collection item belongs to.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/net/obop/v01"/>
        <rdfs:label xml:lang="en">belongsToCollection</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/obop/belongsToVisual -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/obop/belongsToVisual">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/net/obop/VisualElement"/>
        <rdfs:comment xml:lang="en-gb">The belongsTo property relates a visual element to an interface container it belongs to.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/net/obop/v01"/>
        <rdfs:label xml:lang="en">belongsToVisual</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/obop/connectionHasSource -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/obop/connectionHasSource">
        <rdfs:domain rdf:resource="http://purl.org/net/obop/Connection"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment xml:lang="en">This property relates a Connection element to an individual representing the source of an ObjectProperty. That would be the subject of the ObjectProperty created during runtime.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/net/obop/v01"/>
        <rdfs:label xml:lang="en">connectionHasSource</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/obop/connectionHasTarget -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/obop/connectionHasTarget">
        <rdfs:domain rdf:resource="http://purl.org/net/obop/Connection"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment xml:lang="en">The connectionHasTarget property relates a Connection element to an individual which is an object of an ObjectProperty.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/net/obop/v01"/>
        <rdfs:label xml:lang="en">connectionHasTarget</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/obop/containsDatatype -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/obop/containsDatatype">
        <rdfs:comment xml:lang="en-gb">The containsDatatype relates a field to a DataProperty which should be inserted into the field. In this way, the domain ontology is related to the graphical user interface</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/net/obop/v01"/>
        <rdfs:label xml:lang="en">contains datatype</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/obop/executesAction -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/obop/executesAction">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://purl.org/net/obop/Action"/>
        <rdfs:comment xml:lang="en">An Action is directly executed when the control flow reaches it. In contrast to the activatesAction property, this property is used to specify that the action is immediately performed when this property is reached in the control flow.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/net/obop/v01"/>
        <rdfs:label xml:lang="en">executesAction</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/obop/generatedAccordingToModel -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/obop/generatedAccordingToModel">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/net/obop/v01"/>
        <rdfs:label xml:lang="en">generated from the model</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/obop/hasBranching -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/obop/hasBranching">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://purl.org/net/obop/Branching"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/net/obop/v01"/>
        <rdfs:label xml:lang="en">hasBranching</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/obop/hasCondition -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/obop/hasCondition">
        <rdfs:domain rdf:resource="http://purl.org/net/obop/Branching"/>
        <rdfs:range rdf:resource="http://purl.org/net/obop/Condition"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/net/obop/v01"/>
        <rdfs:label xml:lang="en">hasCondition</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/obop/hasConnection -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/obop/hasConnection">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://purl.org/net/obop/Connection"/>
        <rdfs:comment xml:lang="en">The hasConnection relates an Action element to a Connection element that will be established between two instances in runtime.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/net/obop/v01"/>
        <rdfs:label xml:lang="en">hasConnection</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/obop/hasConnector -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/obop/hasConnector">
        <rdfs:domain rdf:resource="http://purl.org/net/obop/Connection"/>
        <rdfs:range rdf:resource="http://purl.org/net/obop/Connector"/>
        <rdfs:comment> Each connector specifies an object property that links two instances in a knowledge graph. A single connection may consist of multiple connectors and that is presented with this property&quot;.</rdfs:comment>
        <rdfs:label xml:lang="en">hasConnector</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/obop/hasDestinationOfDependency -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/obop/hasDestinationOfDependency">
        <rdfs:domain rdf:resource="http://purl.org/net/obop/Dependency"/>
        <rdfs:comment>This object property points to the object side of the dependecy statement. This is especially useful for expressing rdfs:subClassOf inference rules as this represents the object of  rdfs:subClassOf inference rule.</rdfs:comment>
        <rdfs:seeAlso>hasDestinationOfDependency</rdfs:seeAlso>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/obop/hasFieldValues -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/obop/hasFieldValues">
        <rdfs:domain rdf:resource="http://purl.org/net/obop/ListField"/>
        <rdfs:comment xml:lang="en">The property contains values that can be chosen in graphical control elements such as drop down lists or multiple choice lists.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/net/obop/v01"/>
        <rdfs:label xml:lang="en">hasFieldValues</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/obop/hasLoop -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/obop/hasLoop">
        <rdfs:range rdf:resource="http://purl.org/net/obop/Loop"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/net/obop/v01"/>
        <rdfs:label xml:lang="en">hasLoop</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/obop/hasSequence -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/obop/hasSequence">
        <rdfs:isDefinedBy rdf:resource="http://purl.org/net/obop/v01"/>
        <rdfs:label xml:lang="en">hasSequence</rdfs:label>
        <vs:term_status>testing</vs:term_status>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/obop/hasSourceOfDependency -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/obop/hasSourceOfDependency">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/net/obop/Dependency"/>
        <rdfs:comment xml:lang="en">This object property points to the left side of the dependecy statement. This is especially useful for expressing rdfs:subClassOf inference rules as this represents the left side of  rdfs:subClassOf</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/obop/isFalse -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/obop/isFalse">
        <rdfs:domain rdf:resource="http://purl.org/net/obop/Condition"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/net/obop/v01"/>
        <rdfs:label xml:lang="en">false</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/obop/isRelatedToTargetOntologyInstance -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/obop/isRelatedToTargetOntologyInstance">
        <rdfs:domain rdf:resource="http://purl.org/net/obop/VisualElement"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment xml:lang="en-gb">This object property relates a Visual element to a concrete individual of the corresponding target ontology.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/net/obop/v01"/>
        <rdfs:label xml:lang="en">related to named individual</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/obop/isTrue -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/obop/isTrue">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/net/obop/Condition"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/net/obop/v01"/>
        <rdfs:label xml:lang="en">true</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/obop/modelBelongsTo -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/obop/modelBelongsTo">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://purl.org/net/obop/FunctionalContainer"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/net/obop/v01"/>
        <rdfs:label xml:lang="en">model belongs to interface container</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/obop/specifiedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/obop/specifiedBy">
        <rdfs:comment xml:lang="en">This object property specifies that an element has further specification in another element. For example, it is used to connect a property in a SHACL shape to a Field instance that contains a further description of the form element.</rdfs:comment>
        <rdfs:label xml:lang="en">specifiedBy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/obop/targetClass -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/obop/targetClass">
        <rdfs:domain rdf:resource="http://purl.org/net/obop/Block"/>
        <rdfs:comment xml:lang="en">This object property corresponds to the SHACL.targetClass property and it is related to the block. It specifies the type of instance that is generated when the form is submited. All other block (form) elements are often used to define  data properties of this instance.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/net/obop/belongsToModel -->

    <owl:DatatypeProperty rdf:about="http://purl.org/net/obop/belongsToModel">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment xml:lang="en">The property specifies that a given element (e.g., a named individual) belongs to the model part (implant that consists of named individuals belonging to the domain ontologies).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/net/obop/v01"/>
        <rdfs:label xml:lang="en">belongsToModel</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/net/obop/extractFirstOnlyVariable -->

    <owl:DatatypeProperty rdf:about="http://purl.org/net/obop/extractFirstOnlyVariable">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/net/obop/Query"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment xml:lang="en">This property indicates whether only the first variable is taken from the result of the SPARQLQuery.</rdfs:comment>
        <rdfs:label xml:lang="en">extractFirstOnlyVariable</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/net/obop/finalValue -->

    <owl:DatatypeProperty rdf:about="http://purl.org/net/obop/finalValue">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/net/obop/Loop"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:comment xml:lang="en">This property is used to specify the last (final value) in a loop.</rdfs:comment>
        <rdfs:label rdf:resource="http://purl.org/net/obop/v01"/>
        <rdfs:label xml:lang="en">finalValue</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/net/obop/hasInitiator -->

    <owl:DatatypeProperty rdf:about="http://purl.org/net/obop/hasInitiator">
        <rdfs:domain rdf:resource="http://purl.org/net/obop/Action"/>
        <rdfs:comment xml:lang="en">This property specifies the activity (attribute) in the HTML element that initiates the corresponding action. This is for example &quot;onclick&quot; attribute of the HTML button element. The values are textual names for these attributes in camel case, e.g., &quot;onClick&quot;. There can be more values for the same action.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/net/obop/hasLabel -->

    <owl:DatatypeProperty rdf:about="http://purl.org/net/obop/hasLabel">
        <rdfs:domain rdf:resource="http://purl.org/net/obop/VisualElement"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/net/obop/v01"/>
        <rdfs:isDefinedBy xml:lang="en">This property specifies the label of a visual element that has to stand next to that element. This could be a label next to the input field on a HTML form.</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">hasLabel</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/net/obop/hasPositionNumber -->

    <owl:DatatypeProperty rdf:about="http://purl.org/net/obop/hasPositionNumber">
        <rdfs:domain rdf:resource="http://purl.org/net/obop/Field"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
        <rdfs:comment xml:lang="en">This property specifies the position of an element on a HTML form. 
This is an integer number that represents the ordinary position among all other elements in the corresponding form looking from the upper left corner.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/net/obop/v01"/>
        <rdfs:label xml:lang="en">hasPosition</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/net/obop/hasValue -->

    <owl:DatatypeProperty rdf:about="http://purl.org/net/obop/hasValue">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="en-gb">The hasValue data property relates an instance to a data assigned to that instance. One example is a Condition class.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/net/obop/initialValue -->

